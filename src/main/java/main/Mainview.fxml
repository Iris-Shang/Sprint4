<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="1000.0" prefWidth="598.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Maincontroller">
   <top>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button id="Personbutton" fx:id="Personbutton" mnemonicParsing="false" onAction="#onClickPerson" text="Person page">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button id="Entitybutton" fx:id="Entitybutton" mnemonicParsing="false" onAction="#onClickEntity" prefHeight="45.0" prefWidth="110.0" text="Entities">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button id="Jobbutton" fx:id="Jobbutton" mnemonicParsing="false" onAction="#onClickJob" prefHeight="45.0" prefWidth="76.0" text="Job">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button id="companybutton" fx:id="companybutton" mnemonicParsing="false" onAction="#onClickcompanybutton" text="Company">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button id="skillbutton" fx:id="skillbutton" mnemonicParsing="false" onAction="#onClickskillbutton" text="Skill">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </top>
</BorderPane>
