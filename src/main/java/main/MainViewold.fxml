<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Maincontroller">
   <top>
      <HBox prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button id="personButton" mnemonicParsing="false" onAction="#onClickPerson" text="Person page">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button id="ElistButton" mnemonicParsing="false" onAction="#onClickEntity" text="Entities">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button id="jobpostbutton" mnemonicParsing="false" onAction="#onClickJob" text="Job Post">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </top>
</BorderPane>
